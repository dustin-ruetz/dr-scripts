{
  "name": "dr-scripts",
  "version": "0.0.0-semantically-released",
  "description": "CLI toolbox providing common scripts for Node.js projects.",
  "scripts": {
    "format": "node ./src/ format",
    "lint": "node ./src/ lint",
    "init-repo": "node ./src/ init-repo",
    "test": "node ./src/ test",
    "test:coverage": "node ./src/ test --coverage"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./src/ commit-msg",
      "pre-commit": "node ./src/ pre-commit"
    }
  },
  "homepage": "https://github.com/dustin-ruetz/dr-scripts/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dustin-ruetz/dr-scripts.git"
  },
  "author": "Dustin Ruetz",
  "license": "MIT",
  "bin": {
    "dr-scripts": "./src/index.js"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 6"
  },
  "dependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "arrify": "^2.0.1",
    "babel-jest": "^26.1.0",
    "cosmiconfig": "^6.0.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "doctoc": "^1.4.0",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "is-ci": "^2.0.0",
    "jest": "^26.1.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "lodash.has": "^4.5.2",
    "prettier": "^2.0.5",
    "read-pkg-up": "^7.0.1",
    "which": "^2.0.2",
    "yargs-parser": "^18.1.3"
  }
}
